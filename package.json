{
  "name": "@electronforconstruct/cli",
  "version": "1.2.1",
  "description": "A small utility to manage your Construct Electron projects",
  "scripts": {
    "start": "cross-env NODE_ENV=dev babel-node src/app.js",
    "compileLibs": "rollup -c",
    "prerelease": "yarn run compileLibs",
    "release": "yarn publish --access public"
  },
  "main": "./dist/index.js",
  "preferGlobal": true,
